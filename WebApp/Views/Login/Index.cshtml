    <tr>
        <th>ID</th>
        <th>USername</th>
        <th>PassWord</th>
        <th>Emai</th>
        <th>Addres</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
